{
  "haikus": [
    {
      "author": "",
      "lines": [
        {
          "en": "Steel birds swarm the sky",
          "jp": "鋼鉄の鳥が空に群がる"
        },
        {
          "en": "Scorching breath paints earth to ash",
          "jp": "灼熱の息が大地を灰にする"
        },
        {
          "en": "Silence follows screams.",
          "jp": "叫び声の後に沈黙が続く。"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Brother raised against brother",
          "jp": "兄弟が兄弟に反旗を翻す"
        },
        {
          "en": "Crimson stains the fields of green",
          "jp": "深紅が緑の野を染める"
        },
        {
          "en": "Lost dreams in the dust.",
          "jp": "埃にまみれて失われた夢。"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Cities crumble fast",
          "jp": "都市は急速に崩壊する"
        },
        {
          "en": "Innocent lives lost in rage",
          "jp": "怒りで失われた無垢な命"
        },
        {
          "en": "Hollow hearts remain.",
          "jp": "空洞のハートが残る。"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Shattered homes and lives",
          "jp": "打ち砕かれた家と生活"
        },
        {
          "en": "Hope flickers in the darkness",
          "jp": "暗闇の中で希望が明滅する"
        },
        {
          "en": "Will peace ever bloom?",
          "jp": "平和の花は咲くのだろうか？"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Gentle breeze whispers",
          "jp": "そよ風がささやく"
        },
        {
          "en": "Flowers bloom in fertile soil",
          "jp": "花は肥沃な土壌で咲く"
        },
        {
          "en": "Laughter fills the air.",
          "jp": "笑いが空気に満ちている。"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Open hand outstretched",
          "jp": "広げたオープンハンド"
        },
        {
          "en": "Forgiveness bridges the gap",
          "jp": "許しがギャップを埋める"
        },
        {
          "en": "Two hearts beat as one.",
          "jp": "二つの心臓が一つに鼓動する。"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Dove flies on the wind",
          "jp": "鳩は風に乗って飛ぶ"
        },
        {
          "en": "Peaceful words weave a new song",
          "jp": "平和な言葉が新しい歌を紡ぐ"
        },
        {
          "en": "Unity takes root.",
          "jp": "団結が根付く。"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Sunlight paints the land",
          "jp": "太陽の光が大地を彩る"
        },
        {
          "en": "Children dance in joyful steps",
          "jp": "子供たちは楽しそうにステップを踏んで踊る"
        },
        {
          "en": "Harmony prevails.",
          "jp": "和が勝つ。"
        }
      ]
    },
    {
      "author": "",
      "lines": [
        {
          "en": "Calm seas reflect sky",
          "jp": "穏やかな海が空を映し出す"
        },
        {
          "en": "Tranquility fills the soul",
          "jp": "平穏が魂を満たす"
        },
        {
          "en": "Quiet strength endures.",
          "jp": "静かな強さが耐える。"
        }
      ]
    }
  ]
}
